<!-- Yahoo! Web Analytics - All rights reserved -->
<script type="text/javascript" src="http://d.yimg.com/mi/ywa.js"></script>
<script type="text/javascript">
    var YWATracker = YWA.getTracker('<%=$YAHOO_TRACKER_ID%>');
    <% if tracker_hash%>
		<% tracker_hash.each do |k,v|%>
			YWATracker.setAction('<%=k%>');
			<% if !v.blank?%>
				YWATracker.setDocumentName('<%=v%>');
			<%end%>
			YWATracker.submit();		
		<%end%>
	<%end%>
</script>
<noscript>
    <div>
        <img src="http://a.analytics.yahoo.com/p.pl?a=<%=$YAHOO_TRACKER_ID%>&js=no" width="1" height="1" alt="" />
    </div>
</noscript><!-- End of Yahoo! Web Analytics Code -->
